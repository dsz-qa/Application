<UserControl x:Class="Finly.Pages.ImportPage"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 Height="300" Width="400">
 <Grid>
 <Border Background="{DynamicResource Surface.Background}" BorderBrush="{DynamicResource Surface.Border}" BorderThickness="1" CornerRadius="8" Padding="12" HorizontalAlignment="Center" VerticalAlignment="Center">
 <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
 <TextBlock Text="Import / Sync" FontWeight="SemiBold" FontSize="16" Foreground="{DynamicResource App.Foreground}" HorizontalAlignment="Center" />
 <TextBlock Text="Ta strona jeszcze nie zaimplementowana." Margin="0,8,0,0" Foreground="{DynamicResource App.Foreground}" HorizontalAlignment="Center"/>
 </StackPanel>
 </Border>
 </Grid>
</UserControl>